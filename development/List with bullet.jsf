//==================================================================// SketchKit - List with bullet// Mikko Vartio, Timur Prokopiev - 2012//==================================================================function setDefaultValues() {  var values = new Array();  // Bullet Style  values.push({ name:"Bullet", type:"ComboBox", value:"Arrow,Arrow,Circle,Box,Line,Plus"});// Text  values.push({ name:"Text", type:"text", value:"List item with bullet" });Widget.elem.customData["currentValues"] = values;}function applyCurrentValues() {  var values = Widget.elem.customData["currentValues"];  var style  = values[0].value.split(",")[0].toString();  Widget.GetObjectByName("arrow").visible = (style == "Arrow");  Widget.GetObjectByName("circle").visible = (style == "Circle");  Widget.GetObjectByName("box").visible = (style == "Box");  Widget.GetObjectByName("line").visible = (style == "Line");  Widget.GetObjectByName("plus").visible = (style == "Plus");  Widget.GetObjectByName("label").textChars = values[1].value;}switch (Widget.opCode){	case 1: setDefaultValues();   break;	case 2: applyCurrentValues(); break;}