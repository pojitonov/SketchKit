tByName("location_circle").visible = (type == "Location");  Widget.GetObjectByName("bookmark_circle").visible = (type == "Bookmark");}if (style == "Box") {  Widget.GetObjectByName("twitter_box").visible = (type == "Twitter");  Widget.GetObjectByName("facebook_box").visible = (type == "Facebook");  Widget.GetObjectByName("feed_box").visible = (type == "Feed");  Widget.GetObjectByName("dribbble_box").visible = (type == "Dribbble");  Widget.GetObjectByName("linkedin_box").visible = (type == "Linkedin");  Widget.GetObjectByName("phone_box").visible = (type == "Phone");  Widget.GetObjectByName("email_box").visible = (type == "Email");  Widget.GetObjectByName("location_box").visible = (type == "Location");  Widget.GetObjectByName("bookmark_box").visible = (type == "Bookmark");}}switch (Widget.opCode){	case 1: setDefaultValues();   break;	case 2: applyCurrentValues(); break;}