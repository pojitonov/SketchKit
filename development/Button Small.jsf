//==================================================================// SketchKit - Button Small// Mikko Vartio, Timur Prokopiev - 2012//==================================================================function setDefaultValues() {  var values = new Array();  // Label  values.push({ name:"Label", type:"text", value:"Label" });// Button Style  values.push({ name:"Style", type:"ComboBox", value:"Light,Light,Dark"});// Button State  values.push({ name:"State", type:"ComboBox", value:"Normal,Normal,Hover,Down,Disabled"});Widget.elem.customData["currentValues"] = values;}function applyCurrentValues() {  var values = Widget.elem.customData["currentValues"];  var style  = values[1].value.split(",")[0].toString();  var state  = values[2].value.split(",")[0].toString();  Widget.GetObjectByName("label_normal(l)").visible = false;  Widget.GetObjectByName("bg_normal(l)").visible = false;  Widget.GetObjectByName("label_disabled(l)").textChars = values[0].value;  Widget.GetObjectByName("label_down(l)").textChars = values[0].value;  Widget.GetObjectByName("label_hover(l)").textChars = values[0].value;  Widget.GetObjectByName("label_normal(l)").textChars = values[0].value;  Widget.GetObjectByName("label_disabled(d)").textChars = values[0].value;  Widget.GetObjectByName("label_down(d)").textChars = values[0].value;  Widget.GetObjectByName("label_hover(d)").textChars = values[0].value;  Widget.GetObjectByName("label_normal(d)").textChars = values[0].value;if (style == "Light") {  Widget.GetObjectByName("label_normal(l)").visible = (state == "Normal");  Widget.GetObjectByName("bg_normal(l)").visible = (state == "Normal");  Widget.GetObjectByName("label_hover(l)").visible = (state == "Hover");  Widget.GetObjectByName("bg_hover(l)").visible = (state == "Hover");  Widget.GetObjectByName("label_down(l)").visible = (state == "Down");  Widget.GetObjectByName("bg_down(l)").visible = (state == "Down");  Widget.GetObjectByName("label_disabled(l)").visible = (state == "Disabled");  Widget.GetObjectByName("bg_disabled(l)").visible = (state == "Disabled");}if (style == "Dark") {  Widget.GetObjectByName("label_normal(d)").visible = (state == "Normal");  Widget.GetObjectByName("bg_normal(d)").visible = (state == "Normal");  Widget.GetObjectByName("label_hover(d)").visible = (state == "Hover");  Widget.GetObjectByName("bg_hover(d)").visible = (state == "Hover");  Widget.GetObjectByName("label_down(d)").visible = (state == "Down");  Widget.GetObjectByName("bg_down(d)").visible = (state == "Down");  Widget.GetObjectByName("label_disabled(d)").visible = (state == "Disabled");  Widget.GetObjectByName("bg_disabled(d)").visible = (state == "Disabled");}}switch (Widget.opCode){	case 1: setDefaultValues();   break;	case 2: applyCurrentValues(); break;}