t.GetObjectByName("s5").visible = false;}if (rate == "3") {  Widget.GetObjectByName("u1").visible = false;  Widget.GetObjectByName("u2").visible = false;  Widget.GetObjectByName("u3").visible = false;  Widget.GetObjectByName("u4").visible = true;  Widget.GetObjectByName("u5").visible = true;  Widget.GetObjectByName("s1").visible = true;  Widget.GetObjectByName("s2").visible = true;  Widget.GetObjectByName("s3").visible = true;  Widget.GetObjectByName("s4").visible = false;  Widget.GetObjectByName("s5").visible = false;}if (rate == "4") {  Widget.GetObjectByName("u1").visible = false;  Widget.GetObjectByName("u2").visible = false;  Widget.GetObjectByName("u3").visible = false;  Widget.GetObjectByName("u4").visible = false;  Widget.GetObjectByName("u5").visible = true;  Widget.GetObjectByName("s1").visible = true;  Widget.GetObjectByName("s2").visible = true;  Widget.GetObjectByName("s3").visible = true;  Widget.GetObjectByName("s4").visible = true;  Widget.GetObjectByName("s5").visible = false;}if (rate == "5") {  Widget.GetObjectByName("u1").visible = false;  Widget.GetObjectByName("u2").visible = false;  Widget.GetObjectByName("u3").visible = false;  Widget.GetObjectByName("u4").visible = false;  Widget.GetObjectByName("u5").visible = false;  Widget.GetObjectByName("s1").visible = true;  Widget.GetObjectByName("s2").visible = true;  Widget.GetObjectByName("s3").visible = true;  Widget.GetObjectByName("s4").visible = true;  Widget.GetObjectByName("s5").visible = true;}}switch (Widget.opCode){	case 1: setDefaultValues();   break;	case 2: applyCurrentValues(); break;}